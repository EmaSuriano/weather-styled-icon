{
  "name": "weather-styled-icon",
  "version": "3.1.0",
  "description": "weather-styled-icon React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**",
    "storybook": "start-storybook -p 6006",
    "publish": "npm run build && npm publish",
    "deploy": "build-storybook && gh-pages -d storybook-static"
  },
  "dependencies": {
    "react-test-renderer": "^16.7.0",
    "styled-components": "^4.0.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "16.x"
  },
  "devDependencies": {
    "@storybook/addon-info": "^5.0.1",
    "@storybook/addon-options": "^4.0.0",
    "@storybook/react": "^4.0.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.4.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "gh-pages": "^2.0.0",
    "jest": "^22.3.0",
    "jest-styled-components": "^6.0.0",
    "nwb": "~0.22.0",
    "path": "^0.12.7",
    "raf": "^3.4.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/config/test-setup.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/config/jest.transform.js"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "author": "emanuel.suriano",
  "homepage": "https://emasuriano.github.io/weather-styled-icon/",
  "license": "MIT",
  "repository": "https://github.com/EmaSuriano/weather-styled-icon",
  "keywords": [
    "react-component",
    "weather",
    "icon",
    "styled-component"
  ]
}
