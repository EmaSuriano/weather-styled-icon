{
  "name": "weather-styled-icon",
  "version": "3.3.2",
  "description": "weather-styled-icon React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "start-server-and-test storybook http://localhost:6006 test",
    "lint": "eslint src/**",
    "storybook": "start-storybook -p 6006",
    "storybook-build": "build-storybook"
  },
  "peerDependencies": {
    "react": "16.x",
    "styled-components": "4.x"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "^5.0.6",
    "@storybook/addon-storyshots-puppeteer": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "eslint": "^6.0.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "gh-pages": "^2.0.0",
    "jest": "^25.1.0",
    "nwb": "~0.24.4",
    "path": "^0.12.7",
    "raf": "^3.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.2",
    "react-test-renderer": "^16.8.6",
    "semantic-release": "^17.0.4",
    "start-server-and-test": "^1.7.13",
    "styled-components": "^5.0.1"
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/config/jest.transform.js"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ],
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "author": "emanuel.suriano",
  "homepage": "https://emasuriano.github.io/weather-styled-icon/",
  "license": "MIT",
  "repository": "https://github.com/EmaSuriano/weather-styled-icon",
  "keywords": [
    "react-component",
    "weather",
    "icon",
    "styled-component"
  ],
  "dependencies": {
    "prop-types": "^15.7.2"
  }
}
